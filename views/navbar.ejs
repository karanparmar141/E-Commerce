        }
        header nav ul li a{
            text-decoration: none;
            color: black;
            font-size: 25px;
            font-weight: 700;
            text-transform: capitalize;
            transition: all 0.5s;
        }
        header nav ul li a:hover{
            color: rgba(0, 0, 0, 0.537);
        }
