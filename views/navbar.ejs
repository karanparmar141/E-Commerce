    <style>
        *{
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        .container{
            max-width: 1140px;
            margin: auto;
        }
        header{
            background-color: rgba(255, 255, 0, 0.805);
            padding: 18px 15px;
        }
        header nav ul{
            display: flex;
            justify-content: space-evenly;
        }
